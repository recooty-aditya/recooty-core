{
    "name": "recooty-aditya/recooty-core",
    "description": "This package contains all the centralized requirements for Recooty, such as models, migrations, enums, and services.",
    "authors": [
        {
            "name": "Recooty Inc.",
            "email": "contact@recooty.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Recooty\\Models\\": "src/models",
            "Recooty\\Enums\\": "src/enums",
            "Recooty\\Services\\": "src/services",
            "Recooty\\Traits\\": "src/traits"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/config": "^8.0 || ^9.0 || ^10.0 || ^11.0",
        "illuminate/database": "^8.69 || ^9.27 || ^10.0 || ^11.0",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0 || ^11.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Recooty\\RecootyServiceProvider"
            ]
        }
    }
}
